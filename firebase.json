{
  "hosting": [{
    "target": "moonlet-wallet",
    "public": "hosting",
    "rewrites": [
        {
            "source": "/api/exchangeRates",
            "function": "exchangeRates"
        }
    ]
  }, {
    "target": "moonlet-website",
    "public": "website",
    "ignore": [
      "**/*.php",
      "**/*.json",
      "wp-admin",
      "comments",
      "feed",
      "wpautoterms",
      "category",
      "author",
      "wp-json",
      "wp-content/uploads/WP-STATIC-*",
      "wp-content/uploads/ShortpixelBackups"
    ],
    "rewrites": [
      {
          "source": "/wp-json/**",
          "function": "wpProxy"
      }
  ]
  }]
}
